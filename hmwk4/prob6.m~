A = [-0.038, 18.984, 0, -32.174;
    -0.001, -0.632, 1, 0;
    0, -0.759, -0.518, 0;
    0, 0, 1, 0];

B = [10.1, 0;
    0, -0.0086;
    0.025, -0.011;
    0, 0];
Cont = [B(:,2) A*B(:,2) A*A*B(:,2) A*A*A*B(:,2)]
rank(Cont)

sec = 60;


syms tau
[V,J] = jordan(A);


eJ = [exp(J(1,1)*sec), 0, 0, 0;
    0, exp(J(2,2)*sec), 0, 0;
    0, 0, exp(J(3,3)*sec), 0;
    0, 0, 0, exp(J(4,4)*sec)];

eJtau = [exp(J(1,1)*(sec-tau)), 0, 0, 0;
    0, exp(J(2,2)*(sec-tau)), 0, 0;
    0, 0, exp(J(3,3)*(sec-tau)), 0;
    0, 0, 0, exp(J(4,4)*(sec-tau))];

Aexp_t = vpa(V*eJ*inv(V),2)
Aexp_t_tau = vpa(V*eJtau*inv(V),2)

